<% title="Estate Description" %>
<!-- <%- include('../partials/head', {title}); %> -->

<!DOCTYPE html>
<html lang="fr">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link
      href="https://api.mapbox.com/mapbox-gl-js/v2.4.1/mapbox-gl.css"
      rel="stylesheet"
    />
    <script src="https://api.mapbox.com/mapbox-gl-js/v2.4.1/mapbox-gl.js"></script>
    

    <title><%= title %></title>
  </head>

  <body>
    <nav>
      <a href="/">Accueil</a>
      <a href="/admin/add-estate">Ajouter établissement</a>
      <a href="/user/register">Créer un compte utilisateur</a>
    </nav>
    <h1>Estate Description</h1>
    <div data-estate-lat="<%= estate.lat %>" data-estate-lng="<%= estate.lng %>" id="estateId" style="display: flex"  >
      <h3 style="width: 70%">
        <%= estate %>
      </h3>
      <div>
        <div
          style="width: 400px; height: 400px; border-radius: 12px"
          id="map"
        ></div>
      </div>
    </div>

    <script src="/map.js"></script>
  </body>
</html>
